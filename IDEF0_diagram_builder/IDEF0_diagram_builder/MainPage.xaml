<UserControl x:Class="DiagramBuilder.MainPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:local="clr-namespace:DiagramBuilder.Controls"
    mc:Ignorable="d"
    d:DesignHeight="500" d:DesignWidth="700">

    <Grid x:Name="LayoutRoot" Background="LightGray">
        <local:DiagramBuilderView x:Name="Diagram" VerticalAlignment="Stretch" HorizontalAlignment="Stretch"/>
        
        <!--<Rectangle Width="200" Height="200" Fill="Black"/>-->
    </Grid>
</UserControl>
